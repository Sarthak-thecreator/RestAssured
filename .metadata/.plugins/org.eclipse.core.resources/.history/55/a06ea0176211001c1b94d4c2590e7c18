package com.lti.handson;

import java.util.Scanner;
public class OccurenceElem 
{
		public static void main(String[] args) {
		Scanner s=new Scanner(System.in);
		int a[]=new int[10];
		int f[]=new int[10];
		int count;	
		System.out.println("Enter the elements of the array:");
		for(int i=0;i<10;i++)
		{
			a[i]=s.nextInt();
			f[i]=-1;
		}	
		for(i=0; i<10; i++)
		{
			count = 1;
			for(int j=i+1;j<10;j++)
			{
				if(a[i]==a[j])
				{
					count++;
					f[j]=0;
				}
			}
			if(f[i]!=0)
			{
				f[i]=count;
			}
		}
		System.out.print(“Frequency of element of array : n”);
		for(i=0; i<10; i++)

		{

			if(f[i] != 0)

			{

				System.out.print(a[i] + ” occurs ” + f[i] + ” times” + “n”);

			}

		}

}
}
